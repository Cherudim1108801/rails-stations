<h1>Admin::Movies#adminmovie</h1>
<p>Find me in app/views/admin/movies/adminmovie.html.erb</p>
<!DOCTYPE html>
<html>
    <head>
        <title>上映映画一覧</title>
        <meta charset="UTF-8">
    </head>
    <body>
        <% @movies.each do |movie|%>
        <thead>
            <tr>
                <th>ID</th>
                <th>タイトル</th>
                <th>公開年</th>
                <th>概要</th>
                <th>画像URL</th>
                <th>上映中</th>
                <th>登録日時</th>
                <th>更新日時</th>
            </tr>
        </thead>
        <table>
            <% @movies.each do |movie|%>
            <tr>
                <th><%=movie.id%></th>
                <th><%=movie.name%></th>
                <th><%=movie.year%></th>
                <th><%=movie.description%></th>
                <th><%=movie.image_url%></th>
                <th><%=movie.is_showing%></th>
                <th><%=movie.created_at%></th>
                <th><%=movie.updated_at%></th>
            </tr>
            <% end %>
        </table>
    </body>
</html>